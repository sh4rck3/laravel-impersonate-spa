{
    "name": "sh4rck3/laravel-impersonate-spa",
    "description": "Pacote Laravel para personificação de usuários compatível com SPA (Jetstream, Inertia.js, Vue 3, Sanctum)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "impersonation",
        "personificacao",
        "spa",
        "inertia",
        "vue",
        "sanctum",
        "jetstream"
    ],
    "authors": [
        {
            "name": "sh4rck3",
            "email": "sh4rck3@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/auth": "^12.0",
        "illuminate/session": "^12.0",
        "inertiajs/inertia-laravel": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sh4rck3\\LaravelImpersonateSpa\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sh4rck3\\LaravelImpersonateSpa\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sh4rck3\\LaravelImpersonateSpa\\Providers\\ImpersonateServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    }
}